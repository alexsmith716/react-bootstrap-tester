{
  "passPerPreset": true,
  "presets": [
    {
      "plugins": [
        "babel-plugin-transform-runtime",
      ]
    },
    {
      "passPerPreset": false,
      "presets": [
        "react",
        ["es2015", { 
          modules: false
        }],
        "stage-0",
        "flow"
      ]
    }
  ],
  "plugins": [
    "react-hot-loader/babel",
    "babel-plugin-transform-decorators-legacy",
    "babel-plugin-transform-regenerator",
    "babel-plugin-transform-runtime",
    "babel-plugin-transform-es2015-modules-commonjs",
  ],
}