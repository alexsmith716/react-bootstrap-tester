  "version": "0.0.1",
  "description": "react-bootstrap-tester",
  "main": "index.js",
  "scripts": {
    "start-dev": "export BOOTSTRAPRC_LOCATION=.bootstraprc && NODE_ENV=development BABEL_DISABLE_CACHE=1 nodemon index.js --ignore client/ --ignore webpack/webpack-assets.json",
    "start-prod": "BABEL_DISABLE_CACHE=1 NODE_ENV=production node index.js",
    "bs": "yarn clear && yarn build && yarn build-server && yarn start-prod",
    "build": "yarn clear && yarn build-client && yarn build-server",
    "build-client": "yarn clear-client && export BOOTSTRAPRC_LOCATION=.bootstraprc && NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "build-server": "yarn clear-server && export BOOTSTRAPRC_LOCATION=.bootstraprc && NODE_ENV=production BABEL_DISABLE_CACHE=1 webpack --config ./webpack/webpack.config.server.js",
    "clear": "rimraf build",
    "clear-client": "rimraf public/static/dist/client",
    "clear-server": "rimraf build/server",
    "slate": "rimraf node_modules && yarn install",
    "lint": "eslint client server",
    "test-watch": "jest --watch",
    "test": "jest",
    "validate": "npm ls",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "pre-commit": [
    "test",
    "precommit"
  ],
